{
  "name": "veritise-faucet",
  "version": "1.0.0",
  "description": "Veritise faucet web application",
  "author": "",
  "homepage": "https://github.com/veritise/faucet",
  "private": true,
  "engines": {
    "node": "12.18.2"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.ts --watch server",
    "build": "nuxt build",
    "generate": "nuxt-ts generate",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register server/index.ts",
    "bootstrap-start": "symbol-bootstrap start -p bootstrap -c bootstrap-preset.yml --detached --healthCheck -r",
    "bootstrap-stop": "symbol-bootstrap stop",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore . ",
    "prettier:fix": "prettier --write ./server/** ./components/** ./store/** ./layouts/** ./pages/**",
    "style:fix": "npm run prettier:fix && npm run lint && npm run lint:fix",
    "version": "echo $npm_package_version",
    "precommit": "npm run lint",
    "test": "jest",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/44uk/symbol-faucet.git"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.10",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/recaptcha": "^0.6.2",
    "@types/express-serve-static-core": "^4.17.18",
    "@types/lodash": "^4.14.168",
    "@types/node": "^13.13.45",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "bufferutil": "^4.0.3",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "fs": "0.0.2",
    "js-joda": "^1.11.0",
    "lodash": "^4.17.21",
    "net": "^1.0.2",
    "nuxt": "^2.15.3",
    "nuxt-buefy": "^0.3.31",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.1.0",
    "querystring": "^0.2.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "symbol-sdk": "^0.23.3",
    "tls": "0.0.1",
    "tsconfig-paths": "^3.9.0",
    "utf-8-validate": "^5.0.4",
    "utf8": "^3.0.0",
    "vue": "^2.6.12",
    "vue-material-design-icons": "^4.11.0",
    "vue-property-decorator": "^8.5.1",
    "vue-server-renderer": "^2.6.12"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.8.1",
    "@nuxt/typescript-build": "^0.6.7",
    "@nuxtjs/eslint-config": "^2.0.2",
    "@types/babel-core": "6.25.6",
    "@types/body-parser": "^1.19.0",
    "@types/eslint": "^6.8.1",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/express": "^4.17.11",
    "@types/nodemon": "^1.19.0",
    "@types/prettier": "2.0.0",
    "@types/pug": "^2.0.4",
    "@types/stylus": "^0.48.33",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.9",
    "eslint-plugin-node": ">=11.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^6.2.1",
    "jest": "^25.5.4",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sass-loader": "^8.0.2",
    "symbol-bootstrap": "^0.1.2",
    "ts-node": "^8.10.2",
    "url-parse": "^1.5.1",
    "vue-jest": "^3.0.7"
  }
